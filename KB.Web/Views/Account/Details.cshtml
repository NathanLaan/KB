@model KB.Web.Models.AccountDetailsModel

@{
    ViewBag.Title = "KB User - " + @Model.Account.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="span12">
        <h2>@Model.Account.Name</h2>
        <br />
        <b>@Model.TotalEntryCount entries posted. @Model.TotalReplyCount replies posted. @Model.TotalVotesCount votes made.</b>
    </div>
</div>
@foreach (var entity in @Model.EntryList) {
    @Html.DisplayFor(m => entity)
}